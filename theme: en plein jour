    cursor:pointer;
    padding: 0 calc(var(--gutter-size) / 2); 
    flex-grow:1;
}
.legacy-photoset-inner:first-child {
    padding-left:0;
}
.legacy-photoset-inner:last-child {
    padding-right:0;
}
