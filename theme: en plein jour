    padding: calc(var(--gutter-size) / 2) 0; 
    display:flex;
}
.photo-row:first-child {
	padding-top:0;
}
.photo-row:last-child {
	padding-bottom:0;
}
/* legacy-photoset-inner is inside each photo-row,
use inspect firefox or chrome to see more  */
